{
  "extends": [
    "eslint:recommended",
    "eslint-config-prettier"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "impliedStrict": true,
    "ecmaVersion": "latest",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}